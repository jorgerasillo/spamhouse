# GraphQL schema example
#
# https://gqlgen.com/getting-started/

scalar Time

type IPAddress {
  uuid: ID!
  created_at: Time!
  updated_at: Time!
  response_code: String!
  ip_address: String!
}

type IPAddressResult {
  message: String!
  node: [IPAddress!]
}

type Query {
  getIPDetails(input: String!): IPAddressResult!
}

type Mutation{
  enqueue(input: [String!]): IPAddressResult!
}